<form action="/sessions" method="post">
	<input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
	<p>Email:</p>
	<input type="text" name="email">
	<p>Password:</p>
	<input type="text" name="password"><br>
	<br>
	<input type="submit" value="Log In">
</form>

<% if flash[:error] %>
	<p><%= flash[:error] %></p>
<% end %>